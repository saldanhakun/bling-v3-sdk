    "license": "MIT",
    "keywords": [
        "sdk",
        "php",
        "bling",
        "api",
        "v3"
    ],
    "authors": [
        { "name": "Marcelo Saldanha", "email": "saldanha@uttara.com.br", "role": "Developer" },
